{
  "name": "modern-js-stack",
  "version": "1.0.0",
  "license": "ISC",
  "engines": {
    "node": "17.2.0",
    "yarn": "1.22.15"
  },
  "scripts": {
    "start": "npm run dev:start",
    "dev:start": "nodemon -e js,jsx --ignore lib --ignore dist --exec babel-node src/server",
    "dev:wds": "webpack-dev-server --progress",
    "prod:build": "rimraf lib dist && babel src -d lib --ignore .test.js && cross-env NODE_ENV=production webpack --mode production",
    "prod:start": "heroku local",
    "lint": "eslint src webpack.config.babel.js --ext .js,.jsx",
    "test": "eslint src && flow",
    "precommit": "npm test",
    "prepush": "npm test && npm run prod:build",
    "heroku-postbuild": "npm run prod:build"
  },
  "browserslist": [
    "> 1%"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^5.1.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "immutable": "^4.0.0-rc.2",
    "isomorphic-fetch": "^3.0.0",
    "jquery": "^3.6.0",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hot-loader": "^4.12.18",
    "react-jss": "^10.8.2",
    "react-redux": "^7.2.6",
    "react-router": "^6.0.2",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2",
    "redux-actions": "^2.6.5",
    "redux-thunk": "^2.4.0",
    "socket.io": "^4.3.2",
    "socket.io-client": "^4.3.2",
    "tether": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-flow": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "parser": "@babel/eslint-parser",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.3.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-flow-react-proptypes": "^26.0.0",
    "babel-preset-env": "^1.7.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-compat": "^3.13.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fetch-mock": "^9.11.0",
    "flow-bin": "^0.163.0",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "nodemon": "^2.0.15",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "webpack": "^5.63.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.4.0"
  }
}
